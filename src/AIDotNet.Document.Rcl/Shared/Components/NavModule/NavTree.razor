<MList Outlined="Outlined" Shaped="Shaped" Dense="Dense"
       Flat="Flat" Disabled="Disabled" Nav="Nav" Routable="Routable"
       Height="Height" MinHeight="MinHeight" MinWidth="MinWidth"
       MaxHeight="MaxHeight" MaxWidth="MaxWidth" Width="Width">
    <MListItemGroup Color="@Color">
        @foreach (var item in Items)
        {
            <NavItem NavMenu="item" Dense=Dense Mini=Mini ActiveClass="@ActiveClass"/>
        }
    </MListItemGroup>
</MList>
